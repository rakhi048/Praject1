@model List<Customer>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>AccountType</th>
                <th>AccountNumber</th>
                <th>AccessCode</th>
                <th>DepositAmount</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var customer in Model)
            {
                <tr>
                    <td>@customer.Id</td>
                    <td>@customer.Name</td>
                    <td>@customer.AccountType</td>
                    <td>@customer.AccountNumber</td>
                    <td>@customer.AccessCode</td>
                    <td>@customer.DepositAmount</td>
                </tr>
            }
        </tbody>
    </table>
